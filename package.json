{
  "name": "isro-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install-server": "npm istall --prefix server",
    "install-client": "npm istall --prefix client",
    "install": "npm-run-all --parallel  install-server install-client",
    "server": "npm run watch --prefix server",
    "client": "npm start --prefix client",
    "watch": "npm-run-all --parallel server client",
    "deploy": "npm run build --prefix client && npm start --prefix server",
    "deploy-cluster": "npm run build --prefix client && npm run cluster --prefix server",
    "server-test": "npm test --prefix server",
    "client-test": "npm test --prefix client",
    "test": "npm-run-all --parallel server-test client-test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "mongoose": "^8.2.2"
  }
}
